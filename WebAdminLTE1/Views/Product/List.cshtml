@model WebApplication0106.Models.ProductListView
@{
    ViewBag.Title = "작업장별 생산현황";
}
    <h2>작업장별 생산현황</h2>
@foreach (var p in Model.Products)
{
    @Html.Partial("ProductSummary", p)
}
<div class="btn-group pull-right" style="background-color:darkgray;">
    @Html.PageLinks(Model.PagingInfo, x => Url.Action("List", new { page = x, group = Model.CurrentCategory }))
</div>