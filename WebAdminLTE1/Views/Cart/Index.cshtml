@model IEnumerable<WebApplication0106.Models.JobOrder>
@{
    ViewBag.Title = "상세보기";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>상세보기</h2>
<section class="content">
    <table class="table" style="background-color:gainsboro">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Workorderno)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Wc_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Item_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Plan_Qty)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Plan_Date)
            </th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Workorderno)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Wc_Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Item_Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Plan_Qty)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Plan_Date)
                </td>
            </tr>
        }

    </table>
</section>

