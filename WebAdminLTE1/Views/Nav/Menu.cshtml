@model IEnumerable<string>

    <ul class="treeview-menu">
        <li>@Html.ActionLink("전체", "Product", "List")</li>
        @foreach (var link in Model)
        {
            <li>
                @Html.RouteLink(link, new
           {
               controller = "Product",
               action = "List",
               category = link,
               page = 1
           })

            </li>
        }
    </ul>
